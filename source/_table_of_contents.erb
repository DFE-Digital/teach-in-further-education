<div id="<%= data.id %>" class="app-table-of-contents" role="navigation" aria-labelledby="contents-list-heading">
  <h2 class="govuk-heading-m" id="contents-list-heading"><%= data.title %></h2>
  <ul class="govuk-list">
    <% page.blocks.each_with_index do |blk, i| %>
      <% if i > index %>
        <% if blk.show_title %>
          <li class="govuk-heading-s" onclick="window.site.analytics.internalNavigation('<%= "#block_#{i}" %>', '<%= blk.title %>')">- <a class="govuk-link" href="<%= "#block_#{i}" %>"><%= blk.title %></a></li>
        <% end %>
      <% end %>
    <% end %>
  </ul>
</div>