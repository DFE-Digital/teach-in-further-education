<div id="<%= data.id %>" class="app-columns-wrapper" role="region" aria-label="<%= data.title %>">
    <h2 class="govuk-heading-l"><%= data.title %></h2>
    <div class="app-columns">
        <% data.blocks.each_with_index do |b, i| %>
            <%= partial "block", :locals => { :block => b, :index => i, :page => data.page } %>
        <% end %>
    </div>
</div>
